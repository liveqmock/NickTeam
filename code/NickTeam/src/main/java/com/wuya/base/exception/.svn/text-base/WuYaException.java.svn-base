package com.wuya.base.exception;

/**
 * [无涯异常信息封装类]
 * 
 * @author nick
 * @version v1.0 2015-1-24
 */
public class WuYaException extends RuntimeException {

	/**
	 * [对象序列化标识]
	 */
	private static final long serialVersionUID = -2464893757856313592L;

	/**
	 * [错误信息]
	 */
	private String msg;

	/* 无参构造函数 */
	public WuYaException(String msg) {
		this.msg = msg;
	}

	public String getMsg() {
		return msg;
	}

	public void setMsg(String msg) {
		this.msg = msg;
	}

}

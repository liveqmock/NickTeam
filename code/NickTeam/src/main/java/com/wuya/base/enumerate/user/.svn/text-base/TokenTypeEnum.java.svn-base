package com.wuya.base.enumerate.user;

/**
 * [通行证类型－枚举]
 * 
 * @author nick
 * @version v1.0 2015-1-26
 */
public enum TokenTypeEnum {

	USERNAME("username"), MOBILE("mobile"), EMAIL("email");

	private String code;

	private TokenTypeEnum(String code) {
		this.code = code;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

}

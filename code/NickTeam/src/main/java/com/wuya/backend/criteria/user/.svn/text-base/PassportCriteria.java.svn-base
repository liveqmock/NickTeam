package com.wuya.backend.criteria.user;

import com.wuya.base.criteria.BaseCriteria;

/**
 * [通行证－查询类]
 * 
 * @author nick
 * @version v1.0 2015-2-3
 */
public class PassportCriteria extends BaseCriteria {

	/**
	 * [对象序列化标识]
	 */
	private static final long serialVersionUID = -111578192780919102L;

	/**
	 * [用户名－like匹配]
	 */
	private String lusername;

	/**
	 * [邮箱－like匹配]
	 */
	private String lemail;

	/**
	 * [手机－like匹配]
	 */
	private String lmobile;

	/**
	 * [类型]
	 */
	private String type;

	/**
	 * [领域]
	 */
	private String domain;

	public String getLusername() {
		return lusername;
	}

	public void setLusername(String lusername) {
		this.lusername = lusername;
	}

	public String getLemail() {
		return lemail;
	}

	public void setLemail(String lemail) {
		this.lemail = lemail;
	}

	public String getLmobile() {
		return lmobile;
	}

	public void setLmobile(String lmobile) {
		this.lmobile = lmobile;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getDomain() {
		return domain;
	}

	public void setDomain(String domain) {
		this.domain = domain;
	}

	@Override
	public String toString() {
		return "PassportCriteria [lusername=" + lusername + ", lemail="
				+ lemail + ", lmobile=" + lmobile + ", type=" + type
				+ ", domain=" + domain + "]";
	}

}
